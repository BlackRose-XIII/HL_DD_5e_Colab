      doneif (field[gIsEquip].value = 0)
      field[Bonus].value += 1</eval>
    <child entity="mSpecMagic">
      <bootstrap thing="mShield"></bootstrap>
      </child>
    </thing>
  <thing id="i5CSkoldWe" name="Weapon of the Skold Rune" description="This weapon has been enchanted by using an {i}Ingot of the Skold Rune{/i}.\n While holding it you are granted +1 to AC." compset="MagicWep" buytemplate="BuyMoney" xactspecial="2">
    <fieldval field="miTypeDet" value="any two-handed melee weapon"/>
    <tag group="ItemRarity" tag="Uncommon"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <eval phase="Final" priority="20000"><![CDATA[
      ~restrict the list of weapons to valid ones only
      gizmo.child[gCustMagic].field[gItemExpr].text = #magicgearexpr[] & " & (wProperty.TwoHanded) & (wCategory.Melee)"

      ~set our name based on the type of weapon chosen
      perform gizmo.findchild[BaseWep].setfocus
      if (state.isfocus <> 0) then
        field[livename].text = titlecase(focus.field[name].text)
      else
        field[livename].text = "Weapon"
        endif
      field[livename].text &= " of the Skold Rune"
      field[shortname].text = field[livename].text
      field[sbName].text = field[livename].text]]>
      <after name="gItemExpr Set"/>
      </eval>
    <eval phase="PreLevel" priority="10000" index="2">~grant bonus to AC
      doneif (field[gIsEquip].value = 0)
      hero.child[ArmorClass].field[Bonus].value += 1</eval>
    <child entity="gMagWeapon"></child>
    </thing>
  <thing id="ma5CIldArm" name="Armor of the Ild Rune" description="While attuned, you have resistance to cold damage." compset="MagicArmor" buytemplate="BuyMoney" xactspecial="2">
    <fieldval field="miTypeDet" value="light, medium, or heavy"/>
    <tag group="ItemRarity" tag="Rare" name="Rare" abbrev="Rare"/>
    <tag group="Helper" tag="NeedAttune" name="Requires Attunement" abbrev="Requires Attunement"/>
    <tag group="Helper" tag="ShowSpec"/>
    <eval phase="Final" priority="10000" index="2"><![CDATA[      ~set our name based on the type of armor chosen
      perform gizmo.findchild[BaseArmor].setfocus
      if (state.isfocus <> 0) then
        field[livename].text = titlecase(focus.field[name].text)
      else
        field[livename].text = "Armor"
        endif
      field[livename].text &= " of the Ild Rune"
      field[shortname].text = field[livename].text
      field[sbName].text = field[livename].text]]></eval>
    <eval phase="PreLevel" priority="10000">doneif (field[gIsAttuned].value = 0)
      doneif (field[gIsEquip].value = 0)
perform hero.assign[DamageRes.dtCold]</eval>
    <child entity="gMagArmor"></child>
    </thing>
  <thing id="i5CIldWeap" name="Weapon of the Ild Rune" description="The weapon has been enchanted by using a {i}Opal of the Ild Rune{/i}.\n\nIt deals an extra 1d6 fire damage when it strikes a target." compset="MagicWep" buytemplate="BuyMoney" xactspecial="2">
