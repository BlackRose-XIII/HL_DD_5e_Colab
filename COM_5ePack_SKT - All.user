  <thing id="ma5CSkoldS" name="Shield of the Skold Rune" description="This is a shield that has been created by using the {i}Ingot of the Skold Rune{/i}. \n\nThe first time after each of your long rests that damage reduces you to 0 hit points, you are instead reduced to 1 hit point. You must be weilding the shield for this to function." compset="MagicArmor">
    <fieldval field="miTypeDet" value="shield"/>
