    <fieldval field="rHeightMin" value="60"/>
    <fieldval field="rSpeed" value="25"/>
    <fieldval field="rAC" value="1"/>
    <usesource source="5eHBUrsiCP" parent="UserParent" name="Ursine Player Race"/>
