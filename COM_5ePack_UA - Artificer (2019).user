    </thing>
  <thing id="cArR5CRetWep" name="Returning Weapon" description="{i}{b}Item:{/b} A simple or martial weapon with the thrown\nproperty{/i}\n\nThis magic weapon grants a +1 bonus to attack and damage rolls made with it, and it returns to the wielder’s hand immediately after it is used to make a ranged attack.\n\n{i}Note: Use the Returning Weapon adjustment tp apply this infusion to a weapon{/i}" compset="CustomSpec" uniqueness="unique">
    <fieldval field="abDuration" value="Indefinitely; see description"/>
    <tag group="abRange" tag="Touch"/>
    <tag group="abAction" tag="Action"/>
    <tag group="SpecSource" tag="cHelpArR"/>
    <tag group="Helper" tag="Secondary"/>
    </thing>
  <thing id="p5CArRResArm" name="Infusion: Resistant Armor" description="{i}{b}Item:{/b} A suit of armor or a shield{/i}\n\nWhile wearing this armor, a creature has resistance to one of the following damage types, which you choose when you infuse the item:\nacid, cold, fire, force, lightning, necrotic, poison, psychic, radiant, or thunder.\n\n{i}Note: Use the Enhnace Defense adjustment tp apply this infusion to a suit of armor or a shield.{/i}\n\n      ~set our name based on the type of armor chosen\n      perform gizmo.findchild[BaseArmor].setfocus\n      if (state.isfocus &lt;&gt; 0) then\n        field[livename].text = titlecase(focus.field[name].text)\n      else\n        field[livename].text = &quot;Armor&quot;\n        endif\n      field[livename].text &amp;= &quot; of Resistance&quot;\n      field[shortname].text = field[livename].text\n      field[sbName].text = field[livename].text" compset="InPlay">
    <comment>titlecase{sName)</comment>
    <fieldval field="pMinimum" value="1"/>
    <fieldval field="pMaximum" value="2"/>
    <fieldval field="pCandExpr2" value="component.DamageType &amp; (DamageRes.dtAcid | DamageRes.dtCold | DamageRes.dtFire | DamageRes.dtForce | DamageRes.dtLightnin | DamageRes.dtNecrotic | DamageRes.dtPoison | DamageRes.dtPsychic | DamageRes.dtRadiant | DamageRes.dtThunder)"/>
    <fieldval field="pCandExpr" value="component.BaseArmor &amp; !EquipType.Shield&amp; !Helper.EquipMag &amp; !Helper.Natural"/>
    <usesource source="p5eArtV2UACP"/>
    <tag group="Helper" tag="AdjInfo"/>
    <tag group="Helper" tag="NoIncr"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <tag group="OthAdjCat" tag="Class"/>
    <tag group="Helper" tag="AdjNoPlus"/>
    <eval phase="Final" priority="5000" index="2"><![CDATA[
     ~ If we're not enabled, get out now
     doneif (field[pIsOn].value = 0)
     ~ If nothing's been chosen, get out now
     doneif (field[pChosen].ischosen = 0)
     doneif (field[pChosen2].ischosen = 0)
      
      perform field[pChosen2].chosen.pulltags[DamageRes.?]
      perform field[pChosen].chosen.assign[Helper.EquipMag]
      perform hero.pushtags[DamageRes.?]]]></eval>
    <eval phase="Render" priority="999999999999"><![CDATA[
     ~ If we're not enabled, get out now
     doneif (field[pIsOn].value = 0)
     ~ If nothing's been chosen, get out now
     doneif (field[pChosen].ischosen = 0)
     doneif (field[pChosen2].ischosen = 0)

     var sName as string

     sName = field[pChosen2].chosen.field[name].text
     field[pChosen].chosen.field[livename].text = field[pChosen].chosen.field[name].text & " of " & sName & " Resistance"]]></eval>
    </thing>
  <thing id="p5CArRRetWep" name="Infusion: Returning Weapon" description="{i}{b}Item:{/b} A simple or martial weapon with the thrown property{/i}\n\nThis magic weapon grants a +1 bonus to attack and damage rolls made with it, and it returns to the wielder’s hand immediately after it is used to make a ranged attack.\n\n{i}Note: Use the Enhnace Weapon adjustment tp apply this infusion to a weapon{/i}" compset="InPlay">
    <fieldval field="pMinimum" value="1"/>
    <fieldval field="pMaximum" value="2"/>
    <fieldval field="pCandExpr" value="component.BaseWep &amp; !Helper.EquipMag &amp; !component.BaseNatWep &amp; wProperty.Thrown"/>
    <usesource source="p5eArtV2UACP"/>
    <tag group="Helper" tag="AdjInfo" name="Adjustment Information" abbrev="Adjustment Information"/>
    <tag group="Helper" tag="AdjNoPlus" name="AdjNoPlus" abbrev="AdjNoPlus"/>
    <tag group="OthAdjCat" tag="Class" name="Class" abbrev="Class"/>
    <tag group="Helper" tag="NoIncr"/>
    <tag group="ChooseSrc1" tag="Hero" name="All Picks on Hero" abbrev="All Picks on Hero"/>
    <eval phase="PostLevel" priority="11000">~ If we&apos;re not enabled, get out now
     doneif (field[pIsOn].value = 0)
     ~ If nothing&apos;s been chosen, get out now
     doneif (field[pChosen].ischosen = 0)

     var sText as string

     field[pChosen].chosen.field[Bonus].value += 1
     sText = &quot;This weapon returns to the wielder’s hand immediately after it is used to make a ranged attack.&quot;
     field[pChosen].chosen.field[livename].text = field[pChosen].chosen.field[name].text &amp; &quot; of Returning&quot;
     field[pChosen].chosen.field[CustDesc].text = field[pChosen].chosen.field[CustDesc].text &amp; &quot;{br}&quot; &amp; sText</eval>
    </thing>
  <thing id="cArR5CAlcMas" name="Alchemical Mastery" description="At 6th level, your command of magical chemicals has become masterful, enhancing the healing and damage you create through them. When you cast a spell using your alchemist’s supplies as the\nspellcasting focus, you gain a bonus to one roll of the spell. That roll must restore hit points or be a damage roll that deals acid or poison damage, and the bonus equals your Intelligence modifier (minimum of +1).\n\nIn addition, you can cast lesser restoration without expending a spell slot, provided you use alchemist’s supplies as the spellcasting focus.\n\nYou can do so a number of times per day equal to your Intelligence modifier (minimum of once)." compset="ClSpecial">
    <bootstrap thing="spLessRest">
      <containerreq phase="First" priority="2500"><![CDATA[fieldval:abValue5 >= 6]]></containerreq>
      <autotag group="Usage" tag="Day"/>
      <assignval field="trkMax" value="1"/>
      </bootstrap>
    <eval phase="PostLevel" priority="2400"><![CDATA[
     doneif (tagis[Helper.ShowSpec] = 0)
     doneif (tagis[Helper.Disable] <> 0)

     var iMod as number
     iMod = hero.child[aINT].field[aModBonus].value

     field[abValue5].value = hero.childfound[cHelpArR].field[cTotalLev].value
     ~hero.childfound[spLessRest].field[trkMax].value = iMod]]></eval>
    </thing>
