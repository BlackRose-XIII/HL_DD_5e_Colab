    </thing>
  <thing id="p5CArRArcWep" name="Arcane Weapon" description="You channel arcane energy into one simple or martial weapon you’re holding, and choose one damage type: acid, cold, fire, lightning, poison, or thunder. Until the spell ends, you deal an extra 1d6 damage of the chosen type to any target you hit with the weapon. If the weapon isn’t magical, it becomes a magic weapon for the spell’s duration.\n\nAs a bonus action, you can change the damage type, choosing from the options above.\n\n{b}At Higher Levels.{/b} When you cast this spell using a spell slot of 3rd level or higher, you can maintain your concentration on the spell for up to 8 hours." compset="InPlay">
    <comment>field[pChosen].chosen.field[livename].text</comment>
    <fieldval field="pDuration" value="Instantaneous"/>
    <fieldval field="pCandExpr" value="component.BaseWep &amp; wProfReq.Simple | wProfReq.Martial"/>
    <fieldval field="pCandExpr2" value="component.DamageType &amp; (DamageRes.dtAcid | DamageRes.dtCold|DamageRes.dtFire|DamageRes.dtLightnin|DamageRes.dtThunder)"/>
    <fieldval field="pSource" value="Artificer spell"/>
    <usesource source="p5eArtV2UACP"/>
    <tag group="OthAdjCat" tag="Spell" name="Spells" abbrev="Spells"/>
    <tag group="ProductId" tag="SoE" name="Shadows of Endore" abbrev="Shadows of Endore"/>
    <tag group="Helper" tag="AdjInfo" name="Adjustment Information" abbrev="Adjustment Information"/>
    <tag group="Helper" tag="NoIncr" name="NoIncr" abbrev="NoIncr"/>
    <eval phase="Render" priority="10000"><![CDATA[    ~ If we're not enabled, get out now
      doneif (field[pIsOn].value = 0)
    ~ If nothing's been chosen, get out now
    doneif (field[pChosen].ischosen = 0)
    ~ If nothing's been chosen, get out now
    doneif (field[pChosen2].ischosen = 0)

    var sText as string
    var sElement as string

    sElement = field[pChosen2].chosen.field[name].text

    sText = " +1d6 " & sElement & " damage."

    field[pChosen].chosen.field[wDamExtra].text = sText]]></eval>
    </thing>
  <thing id="ra5CArRMigMast" name="Might of the Master" description="The following numbers increase by 1 when your proficiency bonus increases by 1: the homunculus’s skill bonuses and the bonuses to hit and damage of its Acidic Spittle." compset="RaceSpec">
    <fieldval field="actIncrNam" value="Artificer Level"/>
    <fieldval field="actUserMin" value="0"/>
    <fieldval field="actUserMax" value="20"/>
    <usesource source="p5eArRUACP"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Helper" tag="UserAdjust"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
     doneif (tagis[Helper.ShowSpec] = 0)
     doneif (tagis[Helper.Disable] <> 0)

     var iLevel as number
     var iBonus as number
     iLevel = field[actUser].value

    if (iLevel >= 17) then
      iBonus = 6
    elseif (iLevel >= 13) then
      iBonus = 5
    elseif (iLevel >= 9) then
      iBonus = 4
    elseif (iLevel >= 5) then
      iBonus = 3
    else
      iBonus = 2
    endif

    herofield[tBonusHP].value += iLevel * 5

    foreach pick in hero from BaseSkill
      eachpick.field[Racial].value += iBonus
      nexteach

    hero.child[wOtherRng].field[BonAlch].value = iBonus - 2]]></eval>
    </thing>
  <thing id="ra5CArRATArtLev" name="Artificer&apos;s Level" description="{i}{b}Note{/b} used to grant the Arcane Turret bonus hit points based on its Artificer master&apos;s level{/i}" compset="RaceSpec">
    <fieldval field="actIncrNam" value="Artificer Level"/>
    <fieldval field="actUserMin" value="0"/>
    <fieldval field="actUserMax" value="20"/>
    <usesource source="p5eArRUACP"/>
    <tag group="Helper" tag="UserAdjust"/>
    <tag group="Helper" tag="ShowSpec"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
     doneif (tagis[Helper.ShowSpec] = 0)
     doneif (tagis[Helper.Disable] <> 0)

     var iLevel as number
     var iBonus as number
     iLevel = field[actUser].value

     herofield[tBonusHP].value += iLevel * 5]]></eval>
    </thing>
