    <tag group="Helper" tag="Secondary"/>
    <exprreq message="Must be a 16th level Artificer"><![CDATA[hero.child[cHelpArR].field[cTotalLev].value >= 16]]></exprreq>
    </thing>
  <thing id="cArR5CResArm" name="Resistant Armor" description="{i}{i}{b}Prerequisite:{/b} 8th-level artificer\n{b}Item:{/b} A suit of armor or a shield{/i}\n\nWhile wearing this armor, a creature has resistance to one of the following damage types, which you choose when you infuse the item:\nacid, cold, fire, force, lightning, necrotic, poison, psychic, radiant, or thunder.\n\n{i}Note: Use the Resistant Armor adjustment tp apply this infusion to a suit of armor.{/i}" compset="CustomSpec" uniqueness="unique">
    <fieldval field="abDuration" value="Indefinitely; see description"/>
    <tag group="Helper" tag="Secondary"/>
    <tag group="SpecSource" tag="cHelpArR"/>
    <tag group="abAction" tag="Action"/>
