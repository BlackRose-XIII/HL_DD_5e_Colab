    <arrayval field="usrArray" index="7" value="Dimensional shackles"/>
    <arrayval field="usrArray" index="8" value="Gem of seeing"/>
    <arrayval field="usrArray" index="9" value="Horn of blasting"/>
    <arrayval field="usrArray" index="10" value="Ring of free action"/>
    <arrayval field="usrArray" index="11" value="Ring of protection"/>
    <arrayval field="usrArray" index="12" value="Ring of the ram"/>
