  <thing id="ra5CMGKHWT" name="Khenra Weapon Training" description="You have proficiency with the khopesh, spear, and javelin.\n\nNote: Khopesh uses the longsword stats." compset="RaceSpec">
    <tag group="WepProf" tag="wLongsword"/>
    <tag group="WepProf" tag="wSpear"/>
    <tag group="WepProf" tag="wJavelin"/>
