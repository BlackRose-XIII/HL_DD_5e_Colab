  <thing id="bt5CMGInP2" name="Sarcastic" description="I use sarcasm and insults to keep a distance between myself and my crop-mates, because I don’t want to get attached to them." compset="BackPerTrt" uniqueness="unique">
    <tag group="Roll" tag="2"/>
    <tag group="Background" tag="bck5CMGIni"/>
    <tag group="Background" tag="bck5CMGXIn"/>
    </thing>
  <thing id="bt5CMGINP3" name="Perfectionist" description="I’ll settle for nothing less than perfection—in myself, in my cropmates, in everything." compset="BackPerTrt" uniqueness="unique">
