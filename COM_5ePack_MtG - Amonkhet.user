    <tag group="Roll" tag="3"/>
    <tag group="Background" tag="bck5CMGXIn"/>
    </thing>
  <thing id="bt5CMGInP4" name="Resolute" description="I’m so focused on the glorious afterlife that nothing in this life can shake my calm resolve" compset="BackPerTrt" uniqueness="unique">
