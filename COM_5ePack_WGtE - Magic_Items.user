  <thing id="io5CEbOrbSh" name="Orb of Shielding" description="An orb of shielding is made from crystal or stone aligned to one of the planes. While you are holding the orb, it shields you from a particular type of energy. Any time you take damage of that type, reduce it by 1d4. The materials and their associated damage types are listed below.\n\n{b}Material                 Damage Type{/b}\nFernian Basalt         Fire\nIrian Quartz             Radiant\nKythrian Skarn        Acid and Poison\nLamannian Flint      Lightning and Thunder\nMabaran Obsidian   Necrotic\nRisian Shale           Cold\nShavarran Chert      Force\nXorian Marble          Psychic" compset="Wondrous">
    <arrayval field="usrArray" index="0" value="Fernian Basalt"/>
    <arrayval field="usrArray" index="1" value="Irian Quartz"/>
    <arrayval field="usrArray" index="2" value="Kythrian Skarn"/>
    <arrayval field="usrArray" index="3" value="Lamannian Flint"/>
    <arrayval field="usrArray" index="4" value="Mabaran Obsidian"/>
    <arrayval field="usrArray" index="5" value="Risian Shale"/>
    <arrayval field="usrArray" index="6" value="Shavarran Chert"/>
    <arrayval field="usrArray" index="7" value="Xorian Marble"/>
    <usesource source="5eRftLWCP"/>
    <usesource source="5eWGtECP"/>
    <tag group="Helper" tag="EquipMag"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="GearType" tag="gtWondrous"/>
    <tag group="Helper" tag="NeedAttune"/>
    <tag group="Custom" tag="5CEbArcFoc"/>
    <tag group="Helper" tag="EquipAvail"/>
    <tag group="User" tag="NoAutoName"/>
    <eval phase="PreLevel" priority="5000"><![CDATA[doneif (field[gIsAttuned].value = 0)

if (field[usrIndex].value = 0) then
   field[abText].text = "+1 to Fire"
elseif (field[usrIndex].value = 1) then
   field[abText].text = "+1 to Radiant"
elseif (field[usrIndex].value = 2) then
   field[abText].text = "+1 to Acid & Poison"
elseif (field[usrIndex].value = 3) then
   field[abText].text = "+1 to Lightning & Thunder"
elseif (field[usrIndex].value = 4) then
   field[abText].text = "+1 to Necrotic"
elseif (field[usrIndex].value = 5) then
   field[abText].text = "+1 to Cold"
elseif (field[usrIndex].value = 6) then
   field[abText].text = "+1 to Force"
elseif (field[usrIndex].value = 7) then
   field[abText].text = "+1 tp Psychic"
   endif

field[livename].text = field[name].text & " (" & field[abText].text & ")"]]></eval>
    </thing>
  <thing id="id5CEbImbRd" name="Imbued Wood Rod" description="An arcane focus made of imbued wood provides a +1 bonus when you roll damage for a spell that inflicts a specific type of damage, as shown below.\n\n{b}Material                     Damage Type{/b}\nFernian Ash                 Fire\nIrian Rosewood            Radiant\nKythrian Manchineel     Acid &amp; Poison\nLamannian Oak           Lightning &amp; Thunder\nMabaran Ebony           Necrotic\nRisian Pine                 Cold\nShavarran Birch           Force\nXorian Wenge              Psychic" compset="Rod">
