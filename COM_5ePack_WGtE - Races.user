    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="abAction" tag="Reaction"/>
    <bootstrap thing="wBite">
      <containerreq phase="First" priority="495"><![CDATA[fieldval:abValue <> 0]]></containerreq>
      <autotag group="Special" tag="MartArts"/>
      <autotag group="abAction" tag="Bonus"/>
      <autotag group="wCategory" tag="Unarmed"/>
      <assignval field="CustDesc" value="Whenever you shift, you can use your elongated fangs to make an unarmed strike as a bonus action. If you hit with your fangs, you can deal piercing damage equal to 1d6 + your Strength modifier, instead of the bludgeoning damage normal for an unarmed strike."/>
      <assignval field="livename" value="Elongated fangs"/>
      <assignval field="wDieCount" value="1"/>
      <assignval field="wDieSize" value="6"/>
      </bootstrap>
    <eval phase="First"><![CDATA[      ~ If we're disabled, do nothing & 
      doneif (tagis[Helper.Disable] = 1)

      doneif (hero.child[ra5CEbShift].field[abilActive].value = 0)

      hero.child[Speed].field[tSpeed].value += 5]]></eval>
    </thing>
  <thing id="sr5CEbWildh" name="Wildhunt" description="Wildhunt shifters are sharp and insightful. \n\nAlert and wary of threats or insightful. Shifters of the Wildhunt are excellent hunters. They tend to be the spiritual leaders of shifter communities." compset="SubRace" uniqueness="unique">
    <fieldval field="rWIS" value="2"/>
    <usesource source="p5eWGtE"/>
    <tag group="RaceSize" tag="Medium0"/>
    <tag group="RaceType" tag="Normal"/>
    <tag group="SubRace" tag="r5CEbShift"/>
    <tag group="ClVaryName" tag="AppParen"/>
    <bootstrap thing="ra5CEbNatTra"></bootstrap>
    <bootstrap thing="ra5CEbMarSce"></bootstrap>
    <bootstrap thing="ra5CEbSFWuldh"></bootstrap>
    </thing>
  <thing id="ra5CEbNatTra" name="Natural Tracker" description="You have proficiency with the Survival skill." compset="RaceSpec">
    <tag group="FeatureTyp" tag="Special" name="Special" abbrev="Special"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <eval phase="First" priority="10000"><![CDATA[
    ~if we've been replaced, get out now
    doneif (tagis[Helper.Disable] <> 0)

    perform hero.assign[ProfSkill.skSurvival]]]></eval>
    </thing>
  <thing id="ra5CEbMarSce" name="Mark the Scent" description="As a bonus action, you can mark one creature you can see within 10 feet of you. Until the end of your next long rest, your proficiency bonus is doubled for any ability check you make to find the marked creature, and you always know the location of that creature if it is within 60 feet of you. You can’t use this trait again until you finish a short or long rest." compset="RaceSpec">
    <fieldval field="abRange" value="10"/>
    <fieldval field="abDuration" value="End of next Long Rest"/>
    <fieldval field="usrChkText" value="Target"/>
    <tag group="abRange" tag="Feet"/>
    <tag group="abAction" tag="Bonus"/>
    <tag group="User" tag="NeedDomain"/>
    <tag group="User" tag="OptDomain"/>
    <tag group="Helper" tag="ActivMenu"/>
    <tag group="User" tag="Activation"/>
    <tag group="Helper" tag="ShowSpec"/>
    </thing>
  <thing id="ra5CEbSFWuldh" name="Shifting Feature (Wildhunt)" description="While shifted, you have advantage on Wisdom checks." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <eval phase="PreAttr" priority="10000"><![CDATA[      ~ If we're disabled, do nothing & 
      doneif (tagis[Helper.Disable] = 1)

      doneif (hero.child[ra5CEbShift].field[abilActive].value = 0)

        #situational[hero.child[svWIS],"Advantage on Wisdom checks.",field[thingname].text]]]></eval>
    </thing>
  <thing id="r5CEbWarfo" name="Warforged" description="The warforged were built by House Cannith  to fight in the Last War.\n\nThe first such creations were mindless automatons, but vast resources were devoted to improving these steel soldiers. \n\nWarforged, though made from wood and metal, can feel pain and emotion. Built as weapons, they must now find a purpose beyond the war.\n\nYour warforged character has the following traits.\n\n{b}Ability Score Increase.{/b} Your Constitution score increases by 1.\n\n{b}Age.{/b} A typical warforged is between two and thirty years old. The maximum lifespan of the warforged remains a mystery; so far, warforged have shown no signs of deterioration due to age.\n\n{b}Alignment.{/b} Most warforged take comfort in order and discipline, tending toward law and neutrality. But some have absorbed the morality — or lack thereof — of the beings with which they served.\n\n{b}Languages.{/b} You can speak, read, and write Common.\n\n{b}Size.{/b} Your size is Medium. Most warforged stand between 5 and 6 1/2 feet tall. Weight and build are affected by subrace.\n\n{b}Speed.{/b} Your base walking speed is 30 feet." compset="Race">
    <fieldval field="rCON" value="1"/>
    <usesource source="p5eWGtE"/>
    <tag group="RaceType" tag="Normal"/>
    <tag group="RaceSize" tag="Medium0"/>
    <bootstrap thing="wUnarmed">
      <autotag group="AttackTarg" tag="1Target"/>
      </bootstrap>
    <bootstrap thing="ra5CEbWarRes"></bootstrap>
    <bootstrap thing="lCommon"></bootstrap>
    <bootstrap thing="ra5CEbSenRes"></bootstrap>
    <bootstrap thing="ra5CEbIntPro"></bootstrap>
    </thing>
  <thing id="ra5CEbWarRes" name="Warforged Resilience" description="You were created to have remarkable fortitude, represented by the following benefits.\n\n- You have advantage on saving throws against being poisoned, and you have resistance to poison damage.\n- You are immune to disease.\n- You don’t need to eat, drink, or breathe.\n- You don’t need to sleep and don’t suffer the effects of exhaustion due to lack of rest, and magic can’t put you to sleep." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="DamageImm" tag="dtDisease" name="Disease" abbrev="Disease"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
      ~if we've been replaced, get out now
      doneif (tagis[Helper.Disable] <> 0)

      perform hero.pushtags[DamageRes.?]

      #situational[hero.child[svAll],"Advantage vs. poison",field[thingname].text]]]></eval>
    <eval phase="PreLevel" priority="10000" index="2"><![CDATA[
      ~if we've been replaced, get out now
      doneif (tagis[Helper.Disable] <> 0)

      perform hero.pushtags[DamageImm.?]]]></eval>
    </thing>
  <thing id="ra5CEbSenRes" name="Sentry’s Rest" description="When taking a long rest, you must spend at least six hours in an inactive, motionless state, rather than sleeping.While doing so, you appear inert, but it doesn’t render you unconscious, and you can see and hear as normal." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    </thing>
  <thing id="ra5CEbIntPro" name="Integrated Protection" description="Your body has built-in defensive layers, which determine your armor class. You gain no benefit from wearing armor, but if you are using a shield, you apply its bonus as normal.\n\nYou can alter your body to enter different defensive modes; each time you finish a long rest, choose one mode to adopt from the Integrated Protection table, provided you meet the mode’s prerequisite.\n\nMode                                                Prerequisite     Effect\nDarkwood Core (unarmored)    None                  11 + your Dexterity modifier (add proficiency bonus if proficient with light armor)\nComposite Plating (armor)         Medium            13 + your Dexterity modifier (maximum of 2) + your proficiency bonus.\nHeavy Plating (armor)                  Heavy                16 + your proficiency bonus; disadvantage on Dexterity (Stealth) checks." compset="RaceSpec">
    <arrayval field="usrArray" index="0" value="Darkwood Core (unarmored)"/>
    <arrayval field="usrArray" index="1" value="Composite Plating (armor)"/>
    <arrayval field="usrArray" index="2" value="Heavy Plating (armor)"/>
    <tag group="Usage" tag="LongRestd"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="abAction" tag="LongRest"/>
    <tag group="abRange" tag="Personal"/>
    <tag group="CalcAC" tag="DexMod"/>
    <tag group="Helper" tag="ActivMenu"/>
    <bootstrap thing="ab5CEbComPla">
      <containerreq phase="First" priority="5000">fieldval:usrIndex = 1</containerreq>
      </bootstrap>
    <bootstrap thing="ab5CEbDarCor">
      <containerreq phase="First" priority="5000">fieldval:usrIndex = 0</containerreq>
      </bootstrap>
    <bootstrap thing="ab5CEbHeaPla">
      <containerreq phase="First" priority="5000">fieldval:usrIndex = 2</containerreq>
      </bootstrap>
    </thing>
  <thing id="ab5CEbDarCor" name="Darkwood Core" description="{i}{b}Prerequiste:{/b} Light armor to gain proficiency bonus on armor class{/i}\n\n11 + your Dexterity modifier (add proficiency bonus if proficient with light armor)" compset="Ability" summary="(unarmored)">
    <tag group="Helper" tag="ShowSpec"/>
    <eval phase="First" priority="5000"><![CDATA[      ~ If we're disabled, do nothing & 
      doneif (tagis[Helper.Disable] = 1)

      hero.childfound[ArmorClass].field[tACBase].value = 11]]></eval>
    <eval phase="Final" priority="10000" index="2"><![CDATA[      ~ If we're disabled, do nothing & 
      doneif (tagis[Helper.Disable] = 1)

      var iDex as number
      iDex = hero.child[aDEX].field[aModBonus].value
      hero.childfound[ArmorClass].field[tACDexMod].value = iDex

      doneif (tagis[ArmProfGrp.ArmorLight] <> 0)

      hero.childfound[ArmorClass].field[tACMisc].value += hero.child[ProfBonus].field[tProfBonus].value]]></eval>
    </thing>
  <thing id="ab5CEbComPla" name="Composite Plating" description="{i}{b}Prerequiste:{/b} Medium armor{/i}\n\n13 + your Dexterity modifier (maximum of 2) + your proficiency bonus." compset="Ability" summary="armor">
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <eval phase="First" priority="5000"><![CDATA[      ~ If we're disabled, do nothing & 
      doneif (tagis[Helper.Disable] = 1)

      hero.childfound[ArmorClass].field[tACBase].value = 13]]></eval>
    <eval phase="Final" priority="10000" index="2"><![CDATA[      ~ If we're disabled, do nothing & 
      doneif (tagis[Helper.Disable] = 1)

      var iDex as number
      iDex = minimum(hero.child[aDEX].field[aModBonus].value,2)
      hero.childfound[ArmorClass].field[tACDexMod].value = iDex

      hero.childfound[ArmorClass].field[tACMisc].value += hero.child[ProfBonus].field[tProfBonus].value]]></eval>
    </thing>
  <thing id="ab5CEbHeaPla" name="Heavy Plating" description="{i}{b}Prerequiste:{/b} Medium armor{/i}\n\n13 + your Dexterity modifier (maximum of 2) + your proficiency bonus." compset="Ability" summary="armor">
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <eval phase="PreAttr" priority="10000"><![CDATA[      ~ If we're disabled, do nothing & 
      doneif (tagis[Helper.Disable] = 1)

      hero.childfound[ArmorClass].field[tACBase].value = 16]]></eval>
    <eval phase="Final" priority="10000" index="2"><![CDATA[      ~ If we're disabled, do nothing & 
      doneif (tagis[Helper.Disable] = 1)

      var iDex as number
      iDex = 0
      hero.childfound[ArmorClass].field[tACDexMod].value = iDex

      hero.childfound[ArmorClass].field[tACMisc].value += hero.child[ProfBonus].field[tProfBonus].value]]></eval>
    </thing>
  <thing id="sr5CEbEnvoy" name="Envoy" description="You were designed, as an envoy,  with a certain specialized function in mind. You might have been designed as an assassin, a healer, or an entertainer, to name a few possibilities. Envoys are the rarest of the warforged subraces, and you could be a unique design.\n\n{b}Ability Score Increase.{/b} Two different ability scores of your choice increase by 1.\n\n{b}Specialized Design.{/b} You gain one skill proficiency of your choice, one tool proficiency of your choice, and fluency in one language of your choice.\n\n{b}Integrated Tool.{/b} Choose one tool you’re proficient with. This tool is integrated into your body, and you double your proficiency bonus for any ability checks you make with it. You must have your hands free to use this integrated tool." compset="SubRace" uniqueness="unique">
    <tag group="RaceType" tag="Normal"/>
    <tag group="RaceSize" tag="Medium0"/>
    <tag group="SubRace" tag="r5CEbWarfo"/>
    <bootstrap thing="ra5CEbASIEnv"></bootstrap>
    <bootstrap thing="ra5CEbSpeDes"></bootstrap>
    <bootstrap thing="ra5CEbDivToo"></bootstrap>
    </thing>
  <thing id="ra5CEbASIEnv" name="Ability Score Increase" description="Choose two ability scores to receive a +1 bonus." compset="RaceSpec" uniqueness="unique">
    <fieldval field="usrCandid1" value="component.BaseAttr"/>
    <fieldval field="usrCandid2" value="component.BaseAttr"/>
    <tag group="Custom" tag="AttrUp" name="AttrUp" abbrev="AttrUp"/>
    <tag group="Helper" tag="SpecUp" name="SpecUp" abbrev="SpecUp"/>
    <tag group="RaceSort" tag="Attribute" name="Attribute" abbrev="Attribute"/>
    <tag group="ChooseSrc1" tag="Hero" name="All Picks on Hero" abbrev="All Picks on Hero"/>
    <tag group="ChooseSrc2" tag="Hero" name="All Picks on Hero" abbrev="All Picks on Hero"/>
    <eval phase="PreAttr" priority="5000"><![CDATA[

      ~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      if (field[usrChosen1].ischosen <> 0) then
        field[usrChosen1].chosen.field[aStartMod].value += 1
        perform field[usrChosen1].chosen.assign[Custom.AttrUp]
        endif

      if (field[usrChosen2].ischosen <> 0) then
        field[usrChosen2].chosen.field[aStartMod].value += 1
        perform field[usrChosen1].chosen.assign[Custom.AttrUp]
        endif]]></eval>
    <evalrule phase="Validation" priority="10000" message="You must choose two different ability scores!" summary="You must choose two different ability scores!"><![CDATA[
      validif (field[usrChosen1].ischosen + field[usrChosen2].ischosen < 2)
      validif (field[usrChosen1].chosen.tagcount[Custom.AttrUp] < 2)
      validif (field[usrChosen2].chosen.tagcount[Custom.AttrUp] < 2)]]></evalrule>
    </thing>
  <thing id="ra5CEbSpeDes" name="Specialized Design" description="You gain one skill proficiency of your choice, one tool proficiency of your choice, and fluency in one language of your choice.\n\n{i}{b}Note:{/b} \n1) Add your chosen tool on the Skills tab under Tool Proficiencies.\n2) Add your chosen Language on the Skills tab under Languages.{/i}" compset="RaceSpec">
    <comment><![CDATA[Does not check if you're selecting a skill you're already proficient with.]]></comment>
    <fieldval field="usrCandid1" value="component.BaseSkill &amp; !Helper.Proficient"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <tag group="Helper" tag="Helper" name="Helper" abbrev="Helper"/>
    <eval phase="First"><![CDATA[
      doneif (tagis[Helper.ShowSpec] = 0)

      doneif (tagis[Helper.Disable] <> 0)

      var tagexpr as string

      if (field[usrChosen1].ischosen <> 0) then
        tagexpr = field[usrChosen1].chosen.tagids[ProfSkill.?," | "]
        ~debug tagexpr
        if (empty(tagexpr) <> 0) then
          ~proficient with tool code
        else
          foreach pick in hero from BaseSkill where tagexpr
            perform eachpick.assign[Helper.Proficient]
          nexteach
        endif
      endif]]></eval>
    </thing>
  <thing id="ra5CEbDivToo" name="Integrated Tool" description="Choose one tool you’re proficient with. This tool is integrated into your body, and you double your proficiency bonus for any ability checks you make with it. You must have your hands free to use this integrated tool." compset="RaceSpec">
    <fieldval field="usrCandid1" value="component.Tool &amp; Proficienc.Tool"/>
    <fieldval field="usrChkText" value="Active"/>
    <tag group="ChooseSrc1" tag="Hero" name="All Picks on Hero" abbrev="All Picks on Hero"/>
    <tag group="Helper" tag="ActivMenu"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <eval phase="PostLevel" priority="15500"><![CDATA[      doneif (tagis[Helper.ShowSpec] = 0)
      doneif (tagis[Helper.Disable] <> 0)
      doneif (field[usrIsCheck].value <> 1)

      if (field[usrChosen1].ischosen <> 0) then
        perform field[usrChosen1].chosen.pulltags[ProfTooDbl.?]
        perform hero.pushtags[ProfTooDbl.?]
      endif]]></eval>
    </thing>
  <thing id="sr5CEbSkirm" name="Skirmisher" description="You were built to scout the edges of battle and outmaneuver your enemies. You are lean and designed for speed.\n\n{b}Ability Score Increase.{/b} Your Dexterity score increases by 2.\n\n{b}Swift.{/b} Your walking speed is increased by 5 feet.\n\n{b}Light Step.{/b} When you are traveling alone for an extended period of time (one hour or more), you can move stealthily at a normal pace. (See chapter 8 of the Player’s Handbook for more information about travel pace.)" compset="SubRace" uniqueness="unique">
    <fieldval field="rDEX" value="2"/>
    <fieldval field="rSpeed" value="35"/>
    <tag group="RaceType" tag="Normal"/>
    <tag group="RaceSize" tag="Medium0"/>
    <tag group="SubRace" tag="r5CEbWarfo"/>
    <bootstrap thing="ra5CEbLigSte"></bootstrap>
    </thing>
  <thing id="sr5CEbJugg" name="Juggernaut" description="As a war machine, You are of an imposing design built for close combat and raw might. You tower over your comrades; standing between 6 and 7 feet in height and weighing up to 450 pounds.\n\n{b}Ability Score Increase.{/b} Your Strength score increases by 2.\n\n{b}Iron Fists.{/b} When you make an unarmed strike, you can deal 1d4 + your Strength modifier bludgeoning damage instead of the normal damage.\n\n{b}Powerful Build.{/b} You count as one size larger when determining your carrying capacity and the weight you can push, drag, or lift." compset="SubRace" uniqueness="unique">
    <fieldval field="rSTR" value="2"/>
    <tag group="RaceSize" tag="Medium0"/>
    <tag group="SubRace" tag="r5CEbWarfo"/>
    <tag group="RaceType" tag="Normal"/>
    <bootstrap thing="wUnarmed">
      <autotag group="AttackTarg" tag="1Target"/>
      <assignval field="livename" value="Iron Fists"/>
      <assignval field="CustDesc" value="When you make an unarmed strike, you can deal 1d4 + your Strength modifier bludgeoning damage instead of the normal damage."/>
      <assignval field="wDieCount" value="1"/>
      <assignval field="wDieSize" value="4"/>
      </bootstrap>
    <bootstrap thing="ra5CEbPowBui"></bootstrap>
    </thing>
  <thing id="ra5CEbPowBui" name="Powerful Build" description="You count as one size larger when determining your carrying capacity and the weight you can push, drag, or lift." compset="RaceSpec">
    <usesource source="p5eWGtE"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="FeatureTyp" tag="Special" name="Special" abbrev="Special"/>
    <eval phase="Final"><![CDATA[    ~if we've been replaced, get out now
    doneif (tagis[Helper.Disable] <> 0)

    herofield[tEncumLgt].value = herofield[tEncumLgt].value * 2
    herofield[tEncumHvy].value = herofield[tEncumHvy].value * 2
    herofield[tEncumMax].value = herofield[tEncumMax].value * 2]]></eval>
    </thing>
  <thing id="ra5CEbLigSte" name="Light Step" description="When you are traveling alone for an extended period of time (one hour or more), you can move stealthily at a normal pace. \n\n(See chapter 8 of the Player’s Handbook for more information about travel pace.)\n\n{i}{b}Travel Pace{/b}\nWhile traveling, a group of adventurers can move at a normal, fast, or slow pace, as shown on the Travel Pace table. The table states how far the party can move in a period of time and whether the pace has any effect. A fast pace makes characters less perceptive, while a slow pace makes it possible to sneak around and to search an area more carefully (see the “Activity While Traveling” section later in this chapter for more information).\n\nNormal: 300-feet/minute, 3-miles/hour, 24-miles/day{/i}" compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    </thing>
  <thing id="ra5CEbMoFind" name="Mark of Finding" description="{b}House:{/b} Tharashk\n\nTraditionally licenses inquisitives and bounty hunters. Recently the Finder’s Guild has expanded into dragonshard prospecting. A talent for finding this resource has given the house new wealth and influence.\n\nTharashk is the youngest of the Dragonmarked Houses and hasn’t embraced all of the customs of the Twelve. Heirs often use family names instead of the house surname.\n\nThe Mark of Finding can appear on either humans or half-orcs.\n\n{b}Guild Specialties:{/b} Half-Orc Bounty Hunting, Investigation, Prospecting" compset="VarRaceTrt" uniqueness="unique">
    <usesource source="p5eWGtE"/>
    <tag group="AllowRCust" tag="rHuman" name="Human" abbrev="Human"/>
    <bootstrap thing="ra5CEbMoFindHu"></bootstrap>
    <bootstrap thing="raDarkVis">
      <autotag group="Value" tag="60"/>
      </bootstrap>
    <bootstrap thing="ra5CEbMoFHI"></bootstrap>
    <bootstrap thing="ra5CEbMoFNV"></bootstrap>
    <bootstrap thing="ra5CEbMoFIP"></bootstrap>
    <eval phase="First" priority="500"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      hero.child[rHuman].field[cSkillMax].value += 1
      perform hero.child[rHuman].assign[AllowSkl1.Any]

      ~overwrite the human's default +1 to everything
      hero.child[rHuman].field[rAttrAll].value = 0]]>
      <before name="Base Race Racial Stats"/>
      </eval>
    </thing>
  <thing id="ra5CEbMoFindHu" name="Ability Score Increase (Human)" description="As a human, your Dexterity and Wisdom scores both increase by 1. In addition, one ability score of your choice increases by 1" compset="RaceSpec" uniqueness="unique">
    <fieldval field="usrCandid1" value="component.BaseAttr"/>
    <tag group="Custom" tag="AttrUp" name="AttrUp" abbrev="AttrUp"/>
    <tag group="Helper" tag="SpecUp" name="SpecUp" abbrev="SpecUp"/>
    <tag group="RaceSort" tag="Attribute" name="Attribute" abbrev="Attribute"/>
    <tag group="ChooseSrc1" tag="Hero" name="All Picks on Hero" abbrev="All Picks on Hero"/>
    <eval phase="PreAttr" priority="5000"><![CDATA[

      ~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      hero.child[aDEX].field[aStartMod].value += 1
      hero.child[aWIS].field[aStartMod].value += 1

      if (field[usrChosen1].ischosen <> 0) then
        field[usrChosen1].chosen.field[aStartMod].value += 1
        perform field[usrChosen1].chosen.assign[Custom.AttrUp]
        endif]]></eval>
    </thing>
  <thing id="ra5CEbMoFindHo" name="Ability Score Increase (Half-Orc)" description="As a half-orc, your Strength and Wisdom scores both increase by 1. In addition, one ability score of your choice increases by 1." compset="RaceSpec" uniqueness="unique">
    <fieldval field="usrCandid1" value="component.BaseAttr"/>
    <tag group="Custom" tag="AttrUp" name="AttrUp" abbrev="AttrUp"/>
    <tag group="Helper" tag="SpecUp" name="SpecUp" abbrev="SpecUp"/>
    <tag group="RaceSort" tag="Attribute" name="Attribute" abbrev="Attribute"/>
    <tag group="ChooseSrc1" tag="Hero" name="All Picks on Hero" abbrev="All Picks on Hero"/>
    <eval phase="PreAttr" priority="5000"><![CDATA[

      ~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      hero.child[aCON].field[aStartMod].value -= 1
      hero.child[aDEX].field[aStartMod].value += 1
      hero.child[aWIS].field[aStartMod].value += 1

      if (field[usrChosen1].ischosen <> 0) then
        field[usrChosen1].chosen.field[aStartMod].value += 1
        perform field[usrChosen1].chosen.assign[Custom.AttrUp]
        endif]]></eval>
    </thing>
  <thing id="ra5CEbMoFHI" name="Hunter’s Intuition" description="Your mark sharpens your senses and helps you find your prey. When you make a Wisdom (Perception) or Wisdom (Survival) check, you can roll one Intuition die, a d4, and add the number rolled to the ability check." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <bootstrap thing="lGoblin"></bootstrap>
    </thing>
  <thing id="ra5CEbMoFNV" name="Nature’s Voice" description="When you reach 3rd level you gain the ability to cast the {i}locate animal or plant{/i} spell, but only as a ritual." compset="RaceSpec">
    <tag group="FeatureTyp" tag="Special" name="Special" abbrev="Special"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="Usage" tag="5CRitual" name=" (Ritual only)"/>
    <bootstrap thing="spLocaAnim">
      <containerreq phase="First" priority="2500"><![CDATA[count:Classes.? >= 3]]></containerreq>
      <autotag group="Usage" tag="Day"/>
      <assignval field="livename" value="Locate animal or plant, (Ritual only)"/>
      <assignval field="sCL" value="2"/>
      </bootstrap>
    </thing>
  <thing id="ra5CEbMoFIP" name="Imprint Prey" description="As a bonus action, choose one creature you can see within 30 feet of you. The target is imprinted in your mind until it dies or you use this trait again. Alternatively, you can imprint a creature as your quarry whenever you succeed on a Wisdom (Survival) check to track it.\n\nWhen tracking your target, double the result of your Intuition die. When your quarry is within 60 feet of you, you have a sense of its location: it can’t be hidden from you, gains no benefit from invisibility, and your attacks against it ignore half cover. Once you use this trait, you cannot use it again until you finish a short or long rest." compset="RaceSpec">
    <fieldval field="abRange" value="30"/>
    <fieldval field="abDuration" value="Until target it dies or you use this trait again."/>
    <fieldval field="trkMax" value="1"/>
    <tag group="abRange" tag="Feet"/>
    <tag group="abAction" tag="Bonus"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Usage" tag="ShortRest"/>
    </thing>
  <thing id="ra5CEbMoFHo" name="Mark of Finding" description="{b}House:{/b} Tharashk\n\nTraditionally licenses inquisitives and bounty hunters. Recently the Finder’s Guild has expanded into dragonshard prospecting. A talent for finding this resource has given the house new wealth and influence.\n\nTharashk is the youngest of the Dragonmarked Houses and hasn’t embraced all of the customs of the Twelve. Heirs often use family names instead of the house surname.\n\nThe Mark of Finding can appear on either humans or half-orcs.\n\n{b}Guild Specialties:{/b} Half-Orc Bounty Hunting, Investigation, Prospecting" compset="VarRaceTrt" uniqueness="unique">
    <usesource source="p5eWGtE" parent="p5eBooks" name="Wayfarer&apos;s Guide to Eberron"/>
    <tag group="AllowRCust" tag="rHalfOrc"/>
    <bootstrap thing="ra5CEbMoFNV"></bootstrap>
    <bootstrap thing="ra5CEbMoFIP"></bootstrap>
    <bootstrap thing="ra5CEbMoFindHo"></bootstrap>
    <bootstrap thing="ra5CEbMoFHI"></bootstrap>
    <eval phase="First" priority="500"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      ~overwrite the half-orc's default ability score bonuses
      hero.child[rHalfOrc].field[rAttrAll].value = 0]]>
      <before name="Base Race Racial Stats"/>
      </eval>
    </thing>
  <thing id="ra5CEbMoDHe" name="Mark of Detection" description="{b}House:{/b} Medani\n\nThe members of House Medani are half-elves with deep roots in Breland. Medani has little interest in the power struggles that sometimes break out between the other Dragonmarked houses. Many Medani heirs are more interested in helping their communities than in raw profit. House Medani heirs often work with local law enforcement or help those who can’t afford their services.\n\n{b}Guild Specialties:{/b} Bodyguards, Investigation, Risk Management" compset="VarRaceTrt" uniqueness="unique">
    <usesource source="p5eWGtE"/>
    <tag group="AllowRCust" tag="rHalfElf"/>
    <tag group="RaReplace" tag="ra5CSkiVe"/>
    <bootstrap thing="ra5CEbMoDHE"></bootstrap>
    <eval phase="First" priority="500"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      ~overwrite the half-orc's default ability score bonuses
      hero.child[rHalfElf].field[rAttrAll].value = 0]]></eval>
    </thing>
  <thing id="ra5CEbMoDHE" name="Ability Score Increase" description="Any one ability score, except Charisma, increases by 1 (your choice)." compset="RaceSpec" uniqueness="unique">
    <fieldval field="usrCandid1" value="component.BaseAttr &amp; !IsAttr.aCHA"/>
    <tag group="Custom" tag="AttrUp" name="AttrUp" abbrev="AttrUp"/>
    <tag group="Helper" tag="SpecUp" name="SpecUp" abbrev="SpecUp"/>
    <tag group="RaceSort" tag="Attribute" name="Attribute" abbrev="Attribute"/>
    <tag group="ChooseSrc1" tag="Hero" name="All Picks on Hero" abbrev="All Picks on Hero"/>
    <tag group="RaReplace" tag="raHElfAbil"/>
    <eval phase="PreAttr" priority="5000"><![CDATA[

      ~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      hero.child[aCON].field[aStartMod].value -= 1
      hero.child[aINT].field[aStartMod].value += 1
      hero.child[aCHA].field[aStartMod].value += 1

      if (field[usrChosen1].ischosen <> 0) then
        field[usrChosen1].chosen.field[aStartMod].value += 1
        perform field[usrChosen1].chosen.assign[Custom.AttrUp]
        endif]]></eval>
    </thing>
  <thing id="cfg5CEbDivPer" name="Divergent Persona" compset="Configure" uniqueness="unique">
    <fieldval field="cToolMax" value="1"/>
    <tag group="AllowTool1" tag="Any"/>
    </thing>
  </document>
