  <thing id="ra5CEbShift" name="Shifting" description="As a bonus action, you can shift into a more bestial form. The shift lasts for 1 minute, until you die, or until you change back as a bonus action. \n\nWhen you shift, you gain temporary hit points equal to your level + your Constitution modifier (minimum of 1 temporary hit point). \n\nYou also gain additional benefits based on your shifter subrace.\n\nOnce you shift, you must finish a short or long rest before you can do it again." compset="RaceSpec">
    <fieldval field="abDuration" value="1"/>
    <fieldval field="trkMax" value="1"/>
    <fieldval field="actName" value="Active"/>
    <tag group="abRange" tag="Personal"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Usage" tag="ShortRest"/>
    <tag group="abAction" tag="Bonus"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="User" tag="Activation"/>
    <tag group="abDuration" tag="Minute"/>
    <bootstrap thing="thp5CEbShtTmpHP"></bootstrap>
    <eval phase="Final" priority="500"><![CDATA[

      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] = 0)

      ~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      field[abValue].value += #totallevelcount[] + hero.child[aCON].field[aModBonus].value

      var temphp as number
      temphp = maximum(field[abValue].value,1)

      hero.childfound[thp5CEbShtTmpHP].field[thpMinAllw].value = temphp
      hero.childfound[thp5CEbShtTmpHP].field[thpMaxAllw].value = temphp
      ~hero.childfound[thp5CEbShtTmpHP].field[thpCurr].value = temphp]]></eval>
    </thing>
  <thing id="thp5CEbShtTmpHP" name="Shifting (Temporary Hit Points)" compset="TempHpPool">
    <eval phase="Final" priority="10000"></eval>
    </thing>
