  <thing id="ra5CEbSFBea" name="Shifting Feature (Beasthide)" description="Whenever you shift, you gain 1d6 additional temporary hit points, and while shifted, you have a +1 bonus to your AC." compset="RaceSpec">
    <fieldval field="actUserMin" value="1"/>
    <fieldval field="actUserMax" value="6"/>
    <fieldval field="actIncrNam" value="+1d6 temporary hit points"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Helper" tag="UserAdjust"/>
    <eval phase="PreAttr" priority="10000"><![CDATA[      ~ If we're disabled, do nothing & 
      doneif (tagis[Helper.Disable] = 1)

      doneif (hero.child[ra5CEbShift].field[abilActive].value = 0)

      hero.childfound[ArmorClass].field[Bonus].value += 1]]></eval>
    <eval phase="Final" priority="600" index="2"><![CDATA[      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] = 0)

      ~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      hero.childfound[thp5CEbShtTmpHP].field[thpMinAllw].value += field[actUser].value
      hero.childfound[thp5CEbShtTmpHP].field[thpMaxAllw].value += field[actUser].value]]></eval>
    </thing>
  <thing id="sr5CEbLongt" name="Longtooth" description="Longtooth shifters are fierce and aggressive, but they form deep bonds with their friends. \n\nMany longtooth shifters have canine traits that become more pronounced as they shift, but they might instead draw on tigers, hyenas, or other predators." compset="SubRace" uniqueness="unique">
