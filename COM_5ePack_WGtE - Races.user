    <tag group="abAction" tag="Bonus"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Usage" tag="ShortRest"/>
    </thing>
  <thing id="sr5CEbMoFHo" name="Mark of Finding" description="{b}House:{/b} Tharashk\n\nTraditionally licenses inquisitives and bounty hunters. Recently the Finderâ€™s Guild has expanded into dragonshard prospecting. A talent for finding this resource has given the house new wealth and influence.\n\nTharashk is the youngest of the Dragonmarked Houses and hasnâ€™t embraced all of the customs of the Twelve. Heirs often use family names instead of the house surname.\n\nThe Mark of Finding can appear on either humans or half-orcs.\n\nThis variant replaces the Half-Orc traits Ability Score Increase, Menacing, Relentless Endurance, and Savage Attacks.\n\n{b}Guild Specialties:{/b} Half-Orc Bounty Hunting, Investigation, Prospecting" compset="VarRaceTrt" uniqueness="unique">
    <usesource source="5eWGtECP"/>
    <tag group="AllowRCust" tag="rHalfOrc"/>
    <tag group="RaReplace" tag="raHOMenace"/>
    <tag group="RaReplace" tag="raHORelEnd"/>
    <tag group="RaReplace" tag="raHOSavAtt"/>
    <bootstrap thing="ra5CEbMoFNV"></bootstrap>
    <bootstrap thing="ra5CEbMoFIP"></bootstrap>
    <bootstrap thing="ra5CEbMoFindHo"></bootstrap>
    <bootstrap thing="ra5CEbMoFHI"></bootstrap>
    <eval phase="First" priority="500"><![CDATA[~ If we're disabled, do nothing
doneif (tagis[Helper.Disable] <> 0)

~overwrite the half-orc's default ability score bonuses
hero.child[rHalfOrc].field[rAttrAll].value = 0]]>
      <before name="Base Race Racial Stats"/>
      </eval>
    </thing>
  <thing id="ra5CEbMoDHE" name="Ability Score Increase" description="Your gain +1 to your Intelligence and Charisma scores. In addition, one ability score of your choice increases by 1." compset="RaceSpec" uniqueness="unique">
    <fieldval field="usrCandid1" value="component.BaseAttr"/>
    <tag group="Custom" tag="AttrUp" name="AttrUp" abbrev="AttrUp"/>
    <tag group="Helper" tag="SpecUp" name="SpecUp" abbrev="SpecUp"/>
    <tag group="RaceSort" tag="Attribute" name="Attribute" abbrev="Attribute"/>
    <tag group="ChooseSrc1" tag="Hero" name="All Picks on Hero" abbrev="All Picks on Hero"/>
    <tag group="RaReplace" tag="raHElfAbil"/>
    <eval phase="PreAttr" priority="5000"><![CDATA[

      ~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      hero.child[aCON].field[aStartMod].value -= 1
      hero.child[aINT].field[aStartMod].value += 1
      hero.child[aCHA].field[aStartMod].value += 1

      if (field[usrChosen1].ischosen <> 0) then
        field[usrChosen1].chosen.field[aStartMod].value += 1
        perform field[usrChosen1].chosen.assign[Custom.AttrUp]
        endif]]></eval>
    </thing>
