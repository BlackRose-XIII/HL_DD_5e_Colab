    <tag group="SubRace" tag="rElf" name="Elf" abbrev="Elf"/>
    <tag group="WeightDice" tag="1" name="1" abbrev="1"/>
    <tag group="WeightSize" tag="4" name="d4" abbrev="d4"/>
    <tag group="ClVaryName" tag="Replace" name="Replace" abbrev="Replace"/>
    <bootstrap thing="ra5CEbAerellf"></bootstrap>
    </thing>
  <thing id="ra5CEbAerellf" name="Aereni Elf" description="An Aereni elf can choose one skill or tool proficiency. Your proficiency bonus is doubled for any ability check you make that uses this chosen proficiency." compset="RaceSpec">
    <fieldval field="abValue" value="1"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="ChooseSrc1" tag="Thing" name="All Things" abbrev="All Things"/>
    <tag group="ChooseSrc2" tag="Thing" name="All Things" abbrev="All Things"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
      ~ If we're disabled, do nothing & 
      doneif (tagis[Helper.Disable] = 1)

      var NumOfProf as number
      ~ Increase or decrease the number of proficiencies 
      NumOfProf = field[abValue].value
      call 5CAddProf]]></eval>
    </thing>
  <thing id="ra5CEbMoPHu" name="Ability Score Increase" description="You gain +2 to your Dexterity score. In addition, one ability score of your choice increases by 1." compset="RaceSpec" uniqueness="unique">
