  <thing id="ra5CAerTrn" name="Aerini Training" description="Choose one skill or tool proficiency. Your proficiency bonus is doubled for any ability check you make that uses the chosen proficiency." compset="RaceSpec">
    <arrayval field="usrArray" index="0" value="Skill"/>
    <arrayval field="usrArray" index="1" value="Tool"/>
    <tag group="Helper" tag="ActivMenu"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="User" tag="NoAutoName"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <eval phase="First"><![CDATA[doneif (tagis[Helper.Disable] <> 0)

perform hero.childfound[rElf].setfocus

if (field[usrIndex].value = 0) then
   focus.field[cSkillMax].value += 1
   perform focus.assign[AllowSkl1.Any]
   field[usrCandid1].text = "(component.BaseSkill & portal.RacSkPrim)"
   if (field[usrChosen1].ischosen <> 0) then
      perform field[usrChosen1].chosen.pulltags[ProfDouble.?]
      perform hero.pushtags[ProfDouble.?]
      endif
else
   focus.field[cToolMax].value += 1
   perform focus.assign[AllowTool1.Any]
   field[usrCandid1].text = "(component.Tool & portal.RacTlPrim)"
   if (field[usrChosen1].ischosen <> 0) then
      perform field[usrChosen1].chosen.pulltags[ProfTooDbl.?]
      perform hero.pushtags[ProfTooDbl.?]
      endif
endif]]></eval>
    </thing>
