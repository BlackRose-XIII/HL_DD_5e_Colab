  <thing id="ra5CElBLeR" name="Legendary Resistance" description="If the creature fails a saving throw, it can choose to succeed instead." compset="RaceSpec">
    <fieldval field="abUserVal" value="3"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="Helper" tag="UserValue" name="Ability that needs a user-set value" abbrev="Ability that needs a user-set value"/>
    <tag group="Universal" tag="Universal" name="Universal Monster Ability" abbrev="Universal Monster Ability"/>
    <tag group="Usage" tag="Day" name="/day" abbrev="/day"/>
    <tag group="User" tag="Tracker" name="tracker" abbrev="tracker"/>
    <tag group="FeatureTyp" tag="Special" name="Special" abbrev="Special"/>
    <eval phase="First" priority="100000">field[trkMax].value += field[abUserVal].value</eval>
    <eval phase="PostLevel" priority="100000" index="2"><![CDATA[
      var cr as number
      var hp as number
      cr = hero.child[cmhHelper].field[cmhCR].value
      if (cr >= 11) then
        hp = 30
      elseif (cr >= 5) then
        hp = 20
      elseif (cr >= 1) then
        hp = 10
        endif
      
      hero.child[cmhHelper].field[cmhHPEffBo].value += hp * field[abUserVal].value]]></eval>
    </thing>
