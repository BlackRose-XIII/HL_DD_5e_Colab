  <thing id="ab5CHsGmSt" name="Gaming Set (see Proficiencies tab)" compset="Ability" uniqueness="unique">
    <tag group="AbilFunc" tag="Background" name="Background Feature" abbrev="Background Feature"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
~ If we're disabled, do nothing & 
doneif (tagis[Helper.Disable] = 1)

var NumOfProf as number
var NumOfTool as number
~ Increase or decrease the number of proficiencies 
NumOfTool = 1
perform hero.childfound[cfg5CProf].assign[AllowTool2.Gaming]
call 5CAddProf]]>
      <before name="Calc skProfBon"/>
      </eval>
    </thing>
